{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "interface-name": [
      false
    ],
    "member-ordering": [
      true,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ],
    "no-console": [false],
    "no-string-literal": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "switch-default": false,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  }
}
